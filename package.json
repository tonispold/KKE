{
  "name": "kke",
  "version": "1.0.0",
  "description": "Kodutoode kontrollija Excelis",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"HomeworkGrader\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Tallinna Ulikooli tudengid",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "electron": "^19.0.4",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "electron-package": "^0.1.0",
    "electron-packager": "^15.5.1"
  }
}
